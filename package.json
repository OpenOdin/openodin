{
    "name": "universeai",
    "version": "0.6.6",
    "description": "The UniverseAI Protocol is a complete package to build decentralized applications with real data interoperability. It is a protocol specification and an implementation of the protocol bundled with an SDK.",
    "keywords": [
        "decentralized",
        "interoperable",
        "interoperability"
    ],
    "homepage": "https://universe.ai",
    "author": "The UniverseAI Foundation",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/universe-ai/universe.git"
    },
    "bugs": {
        "url": "https://github.com/universe-ai/universe/issues"
    },
    "private": false,
    "license": "Apache-2.0",
    "engines": {
        "node": ">=16"
    },
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "scripts": {
        "build": "./node_modules/.bin/bebopc && ./node_modules/.bin/tsc && ./node_modules/.bin/browserify -i ./build/src/datastreamer/FileStreamWriter.js -i ./build/src/datastreamer/FileStreamReader.js ./build/src/datamodel/decoder/thread.js -o ./build/src/datamodel/decoder/thread-browser.js && cp ./build/src/datamodel/decoder/thread.js ./src/datamodel/decoder/thread.js",
        "tsc": "./node_modules/.bin/tsc",
        "clean": "rm -rf ./build",
        "prepublishOnly": "[ -z \"$(git status --porcelain)\" ] && npm run build",
        "c-sdk": "./node_modules/.bin/tsc -p ./tsconfig-sdk.json",
        "pkg-sdk": "npx pkg --targets node14-linux-x64 --output ./build-pkg/linux/cert ./build-sdk/sdk/tools/src/CertCLI.js && npx pkg --targets node14-linux-x64 --output ./build-pkg/linux/keygen ./build-sdk/sdk/tools/src/keygen.js && npx pkg --targets node14-linux-x64 --output ./build-pkg/linux/service ./build-sdk/sdk/tools/src/service.js",
        "test": "./node_modules/.bin/mocha -r ts-node/register --timeout 10000 'test/**/*.mocha.ts'",
        "docs": "./node_modules/.bin/typedoc --entryDocument Home.md --hideBreadcrumbs true --hideInPageTOC true --cleanOutputDir false ./src/*.ts",
        "bebop": "./node_modules/.bin/bebopc",
        "lint": "npx eslint ./src"
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.1",
        "@types/blake2b": "^2.1.0",
        "@types/chai": "^4.3.1",
        "@types/libsodium-wrappers": "^0.7.9",
        "@types/mocha": "^9.1.1",
        "@types/node": "^14.17.21",
        "@types/sql.js": "^1.4.4",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "bebop-tools": "2.4.10",
        "browserify": "^17.0.0",
        "chai": "^4.3.6",
        "eslint": "^8.31.0",
        "mocha": "^10.0.0",
        "pkg": "^5.8.0",
        "regenerator-runtime": "^0.13.9",
        "ts-node": "^10.9.1",
        "typedoc": "~0.22.15",
        "typedoc-plugin-markdown": "~3.11.0",
        "typescript": "4.4.3"
    },
    "dependencies": {
        "bebop": "2.4.10",
        "blake2b": "^2.1.4",
        "eventemitter3": "^4.0.7",
        "libsodium-wrappers": "^0.7.9",
        "pocket-console": "0.5.0",
        "pocket-messaging": "3.0.0",
        "pocket-sockets": "2.0.2",
        "postgresql-client": "^2.5.5",
        "sql.js": "^1.8.0",
        "sqlite3": "^5.1.5",
        "tweetnacl": "^1.0.3",
        "web-worker": "^1.2.0"
    },
    "overrides": {
      "make-dir": "^4.0.0"
    }
}
